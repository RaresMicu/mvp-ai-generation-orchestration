        "json": {
            "workflow": {
                "name": "parallel_verification_flow",
                "entrypoint": "fetch_user",
                "activities": [
                    {
                        "id": "fetch_user",
                        "type": "http_call",
                        "method": "GET",
                        "endpoint": "/users/{id}",
                        "parallelGroup": null,
                        "retryPolicy": null,
                        "timeoutSeconds": null
                    },
                    {
                        "id": "fraud_check",
                        "type": "http_call",
                        "method": "GET",
