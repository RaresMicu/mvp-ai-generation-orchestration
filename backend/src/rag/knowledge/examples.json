                        "retryPolicy": null,
                        "timeoutSeconds": null
                    },
                    {
                        "id": "system_audit",
                        "type": "http_call",
                        "method": "POST",
                        "endpoint": "/audit-logs",
                        "parallelGroup": null,
                        "retryPolicy": null,
                        "timeoutSeconds": null
                    }
                ],
                "dependencies": [
                    {
                        "from": "fetch_user",
                        "to": "fraud_check"
                    },
                    {
                        "from": "fetch_user",
                        "to": "social_enrichment"
                    },
                    {
                        "from": "fraud_check",
                        "to": "system_audit"
                    },
                    {
                        "from": "social_enrichment",
